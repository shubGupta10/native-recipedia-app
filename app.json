{
  "expo": {
    "name": "Recipedia",
    "slug": "recipedia-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/recipediaIcon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/recipediaIcon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shubham720.recipediaapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/recipediaIcon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fef41a58-dc50-43c8-877a-d65e5b4ac242"
      }
    },
    "owner": "shubham720"
  }
}
